NODE_ENV=development
PORT=3001

# OpenAI API key (used by Whisper/STT and optional modules)
OPENAI_API_KEY=your-openai-key-here

# OpenRouter API key
OPENROUTER_API_KEY=your-openrouter-key-here

# OCR model â€” must be a vision-capable model available on OpenRouter
OCR_MODEL=google/gemini-3-flash-preview

# Local TTS (Kokoro 82M)
TTS_MODEL=onnx-community/Kokoro-82M-v1.0-ONNX
TTS_DEFAULT_VOICE=af_nova
TTS_DEVICE=cpu
TTS_DTYPE=q8
TTS_CHUNK_CONCURRENCY=3
TTS_RETRY_ATTEMPTS=2
